@model Survey.ViewModels.WelcomeViewModel


@using (Html.BeginForm())
{
<fieldset>
    <legend>Connection</legend>
    @Html.Label("Login")
    @Html.TextBox("login", Model.Login)
    <br />
    @Html.Label("Password")
    @Html.Password("password")
    <br />
    <input type="submit" value="LogIn" />
</fieldset>
}